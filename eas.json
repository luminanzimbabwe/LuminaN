{
  "cli": {
    "version": ">=5.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "cache": {
        "key": "v1.0.1" 
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "android": {
       
        "buildType": "apk" 
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}