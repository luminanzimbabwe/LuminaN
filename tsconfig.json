{
  "extends": "./node_modules/expo/tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "Node16",
    "module": "Node16",
    "strict": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
